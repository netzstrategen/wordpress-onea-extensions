{
    "name": "netzstrategen/wp-onea-extensions",
    "autoload": {
        "psr-4": {
            "Netzstrategen\\Onea\\": "includes/"
        },
        "files" : [
            "includes/function-helpers.php"
        ]
    },
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "netzstrategen"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.0"
    },
    "require": {
        "psr/container": "^2.0"
    }
}
